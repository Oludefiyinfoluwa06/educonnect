{
  "expo": {
    "name": "educonnect",
    "slug": "educonnect",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "educonnect",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.oftotech.educonnect"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "64f4bdc6-1c4a-47d3-8bd3-355fb12ffa6d"
      }
    }
  }
}
